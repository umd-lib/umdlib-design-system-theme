{# 
/**
 * @file
 * UMD Libraries Table Body component.
 *
 * Available variables:
 * - body_slot: The body slot content.
 */
#}


{% if body_slot is defined %}
  <tr class="table--body-row">
    {% block body_slot %}{% endblock %}
  </tr>
{% else %}
  {# variable error handling #}
    <div class="c-content-interactive-primary c-bg-interactive-primary s-box-small-h"> [ERROR] - UMD Libraries Table Body</div>
    <div class="s-box-small-h"> Required fields missing: <strong>Body Slot (UMD Libraries Table Cell)</strong></div> 
{% endif %}